{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "meson_build",
			"type": "shell",
			"command": "ninja -C build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": ["$gcc"]
		},
		{
            "label": "meson_setup",
            "type": "shell",
            "command": "meson setup build",
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        }
	]
}
